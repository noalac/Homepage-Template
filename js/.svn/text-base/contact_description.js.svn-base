      function showDescription() {
        document.getElementById("contact-photo-description").style.opacity = 1;
        document.getElementById("contact-photo-description").style.filter = "Alpha(opacity=100)";
      }
      function hideDescription() {
        document.getElementById("contact-photo-description").style.opacity = 0;
        document.getElementById("contact-photo-description").style.filter = "Alpha(opacity=0)";
      }

